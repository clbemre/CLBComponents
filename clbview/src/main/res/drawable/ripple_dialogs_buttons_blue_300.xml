<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <item android:state_pressed="true" tools:ignore="StateListReachable">
        <shape>
            <stroke android:width="1dp" android:color="@color/md_blue_300" />
            <solid android:color="@color/md_blue_300" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/md_blue_300" />
            <solid android:color="@color/md_blue_300" />
        </shape>
    </item>

    <item>
        <shape>
            <stroke android:width="1dp" android:color="@android:color/transparent" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</selector>

